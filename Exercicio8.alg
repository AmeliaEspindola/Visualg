algoritmo "Conjunto Numerico"
var n1:inteiro

inicio
  escreva ("digite o numero:")
  leia (n1)
  
  se (n1>0) entao //estrutura de condiçao
  escreval ("Esse numero é positivo")
  fimse
  se (n1<0) entao
  escreval ("Esse numero é negativo")
  fimse
  se (n1=0) entao
  escreva ("Esse numero é zero")
  fimse
// Seção de Comandos 
fimalgoritmo
